import matplotlib.pyplot as plt
import numpy as np

#input data suhu
X = [200,300,400,500,600,700,800,900,1000 ]
#input data seebeck BSe dan BS
SBSe = [1477,1559, 1539 , 1575, 1559, 1582, 1569,  1461, 1257 ]
SBS = [1573.67, 1573.67, 1539.46, 1559.98, 1573.67, 1583.44, 1565.12, 1571.49,  1349.1]

EBSe = [543.83, 473.17, 450.401, 437.842, 431.39, 429.044, 427.223, 425.218, 422.969 ]
EBS  = [778.392, 778.392, 699.186, 661.912,  636.501, 616.968, 601.269, 589.476, 579.504]

KBSe = [23.6824, 34.6546, 44.0888, 52.914, 61.7169, 70.2883, 78.6989, 86.9679, 95.1085]
KBS  = [45.7502, 45.7502, 59.043, 71.8361, 83.6996, 95.0844, 106.148, 116.714, 127.605]

ZTBSe = [100.19, 99.56, 96.79, 102.63, 101.93,  106.94, 106.91, 85.89,70.27]
ZTBS  = [84.26, 126.4, 140.32,113.88, 111, 112.99,113.89, 109.44, 82.64 ]

#PF BSe = []

#ZTBSe = []
#ZTBS  = []

#Plot the data seebeck
fig, ax = plt.subplots(figsize=(14, 10), sharex= True)
plt.plot(X, SBSe, 'p-',color='#e377c2', label='BSe')
plt.plot(X, SBS, 'p-',color='#ff7f0e', label='BS')

plt.xlabel('Suhu(K)',fontsize = 30)
plt.ylabel('Seebeck $\mu$V/K',fontsize = 30)
plt.title('Seebeck $\mu$V/K vs. Suhu (k)',fontsize = 30)
plt.gca().xaxis.set_tick_params(labelsize=14)
plt.gca().yaxis.set_tick_params(labelsize=14)
plt.legend(fontsize = 13)

#Plot the data electrical
fig, ax = plt.subplots(figsize=(14, 10), sharex= True)
plt.plot(X, EBSe, 'p-',color='#e377c2', label='BSe')
plt.plot(X, EBS, 'p-',color='#ff7f0e', label='BS')

plt.xlabel('Suhu(K)',fontsize = 30)
plt.ylabel('Electrical Conductivity $\sigma$/τ($10^{18}$S/ms)',fontsize = 30)
plt.title('Electrical Conductivity $\sigma$/τ($10^{18}$S/ms) vs. Suhu (k)',fontsize = 30)
plt.gca().xaxis.set_tick_params(labelsize=14)
plt.gca().yaxis.set_tick_params(labelsize=14)
plt.legend(fontsize = 13)

#Plot the data termal
fig, ax = plt.subplots(figsize=(14, 10), sharex= True)
plt.plot(X, KBSe, 'p-',color='#e377c2', label='BSe')
plt.plot(X, KBS, 'p-',color='#ff7f0e', label='BS')

plt.xlabel('Suhu(K)',fontsize = 30)
plt.ylabel('Thermal Conductivity $K_e$/τ($10^{14}$W/mKs)',fontsize = 30)
plt.title('Thermal Conductivity $K_e$/τ($10^{14}$W/mKs) vs. Suhu (k)',fontsize = 30)
plt.gca().xaxis.set_tick_params(labelsize=14)
plt.gca().yaxis.set_tick_params(labelsize=14)
plt.legend(fontsize = 13)

#Plot the data zte
fig, ax = plt.subplots(figsize=(14, 10), sharex= True)
plt.plot(X, ZTBSe, 'p-',color='#e377c2', label='BSe')
plt.plot(X, ZTBS, 'p-',color='#ff7f0e', label='BS')

plt.xlabel('Suhu(K)',fontsize = 30)
plt.ylabel('$ZT_{e}$',fontsize = 30)
plt.title('$ZT_{e}$vs. Suhu (k)',fontsize = 30)
plt.gca().xaxis.set_tick_params(labelsize=14)
plt.gca().yaxis.set_tick_params(labelsize=14)
plt.legend(fontsize = 13)

#Plot the power factor
fig, ax = plt.subplots(figsize=(14, 10), sharex= True)
plt.plot(X, ZTBSe, 'p-',color='#e377c2', label='BSe')
plt.plot(X, ZTBS, 'p-',color='#ff7f0e', label='BS')

plt.xlabel('Suhu(K)',fontsize = 30)
plt.ylabel('Power Factor (µW/mK²)',fontsize = 30)
plt.title('Power Factor (µW/mK²) vs Suhu (K)',fontsize = 30)
plt.gca().xaxis.set_tick_params(labelsize=14)
plt.gca().yaxis.set_tick_params(labelsize=14)
plt.legend(fontsize = 13)

#plot merge
fig, ax = plt.subplots(2,2, figsize=(28, 20), sharex= True)
ax[1][0].plot(X, SBSe, 'p-',color='#e377c2', label='BSe')
ax[1][0].plot(X, SBS, 'p-',color='#ff7f0e', label='BS')
ax[1][0].set_xlabel('Suhu(K)',fontsize = 30)
ax[1][0].set_ylabel('Seebeck $\mu$V/K',fontsize = 30)
ax[1][0].set_title('Seebeck $\mu$V/K vs. Suhu (k)',fontsize = 30)
ax[1][0].xaxis.set_tick_params(labelsize=14)
ax[1][0].yaxis.set_tick_params(labelsize=14)
ax[1][0].legend(fontsize = 13)

ax[0][1].plot(X, EBSe, 'p-',color='#e377c2', label='BSe')
ax[0][1].plot(X, EBS, 'p-',color='#ff7f0e', label='BS')
ax[0][1].set_xlabel('Suhu(K)',fontsize = 30)
ax[0][1].set_ylabel('Electrical Conductivity $\sigma$/τ($10^{18}$S/ms)',fontsize = 30)
ax[0][1].set_title('Electrical Conductivity $\sigma$/τ($10^{18}$S/ms) vs. Suhu (k)',fontsize = 30)
ax[0][1].xaxis.set_tick_params(labelsize=14)
ax[0][1].yaxis.set_tick_params(labelsize=14)
ax[0][1].legend(fontsize = 13)

ax[0][0].plot(X, KBSe, 'p-',color='#e377c2', label='BSe')
ax[0][0].plot(X, KBS, 'p-',color='#ff7f0e', label='BS')
ax[0][0].set_xlabel('Suhu(K)',fontsize = 30)
ax[0][0].set_ylabel('Thermal Conductivity $K_e$/τ($10^{14}$W/mKs)',fontsize = 30)
ax[0][0].set_title('Thermal Conductivity $K_e$/τ($10^{14}$W/mKs) vs. Suhu (k)',fontsize = 30)
ax[0][0].xaxis.set_tick_params(labelsize=14)
ax[0][0].yaxis.set_tick_params(labelsize=14)
ax[0][0].legend(fontsize = 13)

ax[1][1].plot(X, ZTBSe, 'p-',color='#e377c2', label='BSe')
ax[1][1].plot(X, ZTBS, 'p-',color='#ff7f0e', label='BS')
ax[1][1].set_xlabel('Suhu(K)',fontsize = 30)
ax[1][1].set_ylabel('$ZT_{e}$',fontsize = 30)
ax[1][1].set_title('$ZT_{e}$ vs. Suhu (k)',fontsize = 30)
ax[1][1].xaxis.set_tick_params(labelsize=14)
ax[1][1].yaxis.set_tick_params(labelsize=14)
ax[1][1].legend(fontsize = 13)

plt.show()
